{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "^(?:/)?(.*)$",
      "dest": "/dist/$1"
    }
  ],
  "functions": {
    "api/proxyWebhook.js": {
      "memory": 512,
      "maxDuration": 30
    }
  }
}
