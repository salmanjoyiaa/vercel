{
  "version": 2,
  "env": [
    "GROQ_API_KEY",
    "GROQ_MODEL",
    "GOOGLE_SHEET_ID",
    "GOOGLE_PRIVATE_KEY",
    "GOOGLE_CLIENT_EMAIL",
    "GCLOUD_PROJECT_ID"
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*.js"
    }
  ],
  "functions": {
    "api/proxyWebhook.js": {
      "memory": 512,
      "maxDuration": 30
    }
  }
}
